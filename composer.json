{
    "name": "gleisonnanet/cep-gratis",
    "description": "Com esse pacote você poderá realizar consultas de CEP gratuitamente.",
    "keywords": ["cep", "correios", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gleison de souza luiz",
            "email": "gleisonnanet@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/dom-crawler": "^3.2",
        "symfony/css-selector": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "gleisonnanet\\CepGratis\\": "src/",
            "gleisonnanet\\CepGratis\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "suggest": {
        "gleisonnanet/cnpj-gratis": "Permite consulta de CNPJ no site da receita",
        "gleisonnanet/cpf-gratis": "Permite consulta de CPF no site da receita"
    },
    "minimum-stability": "stable"
}
